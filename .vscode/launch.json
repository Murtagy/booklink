{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "test.py",
      "type": "python",
      "request": "launch",
      "module": "pytest",
      "justMyCode": true,
      "cwd": "${workspaceFolder}/server",
      "args": ["test.py"]
    },
    {
      "name": "tests",
      "type": "python",
      "request": "launch",
      "module": "pytest",
      "justMyCode": true,
      "cwd": "${workspaceFolder}/server",
      "args": ["."]
    },
    {
      "name": "app.py",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": ["server.apps.app:app"],
      "jinja": true,
      "justMyCode": true,
      "cwd": "${workspaceFolder}"
    }
  ]
}
